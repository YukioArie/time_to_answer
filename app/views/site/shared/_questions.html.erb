<% @questions.each do |question| %>
  <div class="card ml-5 mr-5 mb-5">
    <div class="card-header">
      <%= question.description %>
    </div>
    <div class="card-body">
      <ul>
        <% question.answers.each do |answer|%>
          <li>
            <%= answer.description %>
          </li>
        <% end%>
      </ul>
    </div>
  </div>
<% end%>
<div class="ml-5">
  <%= paginate @questions %>
</div>
